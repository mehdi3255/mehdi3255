using System;

class Program
{
    static void Main()
    {
        Console.Write("Enter a number (1-7) to get the week: ");
        int week;

        bool isValid = int.TryParse(Console.ReadLine(), out week);

        if (!isValid || week < 1 || week > 7)
        {
            Console.WriteLine("Invalid input! Please enter a number between 1 and 7.");
        }
        else
        {
            Console.WriteLine("This is week " + week);
        }
    }
}
